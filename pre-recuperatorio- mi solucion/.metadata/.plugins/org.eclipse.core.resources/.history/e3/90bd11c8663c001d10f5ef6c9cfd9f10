
public class Obra {
private String titulo;
private float duracion;
private Integer calif; //integer aleatorio de 0 a 10 inclusive;


@Override
public String toString() {
	return "Episodio   "+getTitulo() + "  duracion :  "+super.getDuracion()+"   calificacion   " +super.getCalif() +  "+[fechaEmision=" + fechaEmision + "]" ;
}



	
}
