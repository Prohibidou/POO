
public class Pelicula extends Obra {
	private Double recaudacion;
	private String pais;

	
	
public Pelicula(String titulo, float duracion, Integer calif, Double recaudacion, String pais) {
		super(titulo, duracion, calif);
		this.recaudacion = recaudacion;
		this.pais = pais;
	}






@Override
public String toString() {
		return "Pelicula   " + super.getTitulo() + "  duracion :  " + super.getDuracion() + "   calificacion   "
				+ super.getCalif() + "  recaudacion  "+ this.recaudacion+ " pais:  "+ this.pais;

	}

}
