package ej10;

public class Docente extends Personal{
private Integer categoria; //1:simple, 2:Semiexclusiva, 3:exclusiva
	public Docente(String nombre_completo, float antiguedad, float cantHorasTrabMes, float categoria) {
		super(nombre_completo, antiguedad, cantHorasTrabMes);
      this.categoria=categoria;
		
	}

	
	
	

}
