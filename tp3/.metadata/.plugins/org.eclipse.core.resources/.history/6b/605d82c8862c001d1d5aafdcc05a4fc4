package ej10;

public class Personal {
protected String nombre_completo;
protected float antiguedad;
protected float cantHorasTrabMes;
protected float horasAsignadas;
public Personal(String nombre_completo, float antiguedad, float cantHorasTrabMes, float horasAsignadas) {
	super();
	this.nombre_completo = nombre_completo;
	this.antiguedad = antiguedad;
	this.cantHorasTrabMes = cantHorasTrabMes;
	this.horasAsignadas=horasAsignadas;
}



public String getNombre_completo() {
	return nombre_completo;
}
public void setNombre_completo(String nombre_completo) {
	this.nombre_completo = nombre_completo;
}
public float getAntiguedad() {
	return antiguedad;
}
public void setAntiguedad(float antiguedad) {
	this.antiguedad = antiguedad;
}
public float getCantHorasTrabMes() {
	return cantHorasTrabMes;
}
public void setCantHorasTrabMes(float cantHorasTrabMes) {
	this.cantHorasTrabMes = cantHorasTrabMes;
}
	

}
