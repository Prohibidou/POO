package ejercicio5;

public class TortaCoco extends Torta implements TortaBuilder {
	
	public TortaCoco(String relleno, String masa) {
		super(relleno, masa);
	}

	private TortaCoco tortaCoco;
	private String paso4;

	public TortaCoco setPaso1() {
		System.out.println("hago paso 1 coco");
		return this;
	}

	public TortaCoco setPaso2() {
		System.out.println("hago paso 2 coco ");
		return this;
	}

	public TortaCoco setPaso3() {
		System.out.println("hago paso 3 coco ");
		return this;
	}

	public TortaCoco setIngrediente(String ingrediente) {
        this.setIngrediente(ingrediente);
		return this;
	}
	
	public TortaCoco setMasa(String masa) {
        this.setMasa(masa);
		return this;
	}

	@Override
	public TortaCoco setPaso4(String paso4) {
		this.paso4 = paso4;
		return null;
	}
	
	

}