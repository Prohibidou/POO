	public TortaCoco setPaso1() {
		System.out.println("hago paso 1 coco");
		return this;
	}

	public TortaCoco setPaso2() {
		System.out.println("hago paso 2 coco ");
		return this;
	}

	public TortaCoco setPaso3() {
		System.out.println("hago paso 3 coco ");
		return this;
	}

	public TortaCoco getResultado() {
		return new TortaCoco();
	}

	public TortaCoco setIngrediente() {
         
		return this;
	}

	@Override
	public TortaBuilder setPaso4(String paso4) {
		this.paso4 = paso4;
		return null;
	}

}