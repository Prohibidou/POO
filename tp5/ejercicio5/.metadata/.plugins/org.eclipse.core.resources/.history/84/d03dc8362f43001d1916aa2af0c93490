package ejercicio5;

public class TortaCoco implements TortaBuilder {
private TortaCoco tortaCoco;
	public void setPaso1() {
		System.out.println("hago paso 1 coco");
	}

	public void setPaso2() {
		System.out.println("hago paso 2 coco ");
	}

	public void setPaso3() {
		System.out.println("hago paso 3 coco ");
	}

public void setPaso4() {
		
		System.out.println("hago paso 4 coco");
	}

public TortaCoco getResultado()	{
    return this.tortaCoco;
}
	
}